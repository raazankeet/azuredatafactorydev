{
	"name": "fname_mask_test",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_emp_in",
						"type": "DatasetReference"
					},
					"name": "inputFile"
				},
				{
					"dataset": {
						"referenceName": "first_name_lkp",
						"type": "DatasetReference"
					},
					"name": "firstNameLKP"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dummy_op_ds",
						"type": "DatasetReference"
					},
					"name": "maskedFile"
				}
			],
			"transformations": [
				{
					"name": "rowSelection"
				},
				{
					"name": "nameMasking"
				},
				{
					"name": "getIndexBasedName"
				}
			],
			"scriptLines": [
				"parameters{",
				"     seed as integer",
				"}",
				"source(output(",
				"          empID as string,",
				"          name as string,",
				"          country as string,",
				"          salary as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> inputFile",
				"source(output(",
				"          sno as integer,",
				"          Fname as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> firstNameLKP",
				"getIndexBasedName select(mapColumn(",
				"          empID,",
				"          name,",
				"          maskedName = Fname,",
				"          country,",
				"          salary,",
				"          department",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> rowSelection",
				"inputFile derive(maskedIndex = mod(crc32(name),1000)) ~> nameMasking",
				"nameMasking, firstNameLKP lookup(maskedIndex == sno,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> getIndexBasedName",
				"rowSelection sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['maskedfile.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> maskedFile"
			]
		}
	}
}