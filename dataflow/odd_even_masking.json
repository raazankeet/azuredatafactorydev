{
	"name": "odd_even_masking",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_emp_in",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dummy_op_ds",
						"type": "DatasetReference"
					},
					"name": "sink1"
				},
				{
					"dataset": {
						"referenceName": "dummy_op_ds",
						"type": "DatasetReference"
					},
					"name": "sink2"
				}
			],
			"transformations": [
				{
					"name": "addRowCount"
				},
				{
					"name": "split1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empID as string,",
				"          name as string,",
				"          country as string,",
				"          salary as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 keyGenerate(output(record_count as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> addRowCount",
				"derivedColumn1 split(record_count%2==0,",
				"     disjoint: false) ~> split1@(evenFile, oddFile)",
				"addRowCount derive(new_column = sha1(md5(name))) ~> derivedColumn1",
				"split1@evenFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['even.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          empID,",
				"          name,",
				"          country,",
				"          salary,",
				"          department,",
				"          record_count,",
				"          new_column",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink1",
				"split1@oddFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['odd.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          empID,",
				"          name,",
				"          country,",
				"          salary,",
				"          department,",
				"          record_count,",
				"          new_column",
				"     ),",
				"     partitionBy('hash', 1)) ~> sink2"
			]
		}
	}
}