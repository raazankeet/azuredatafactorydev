{
	"name": "odd_even_write_to_file",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_emp_in",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_emp_out",
						"type": "DatasetReference"
					},
					"name": "evenFile",
					"rejectedDataLinkedService": {
						"referenceName": "Lnkd_ankitadlsadf",
						"type": "LinkedServiceReference"
					}
				},
				{
					"dataset": {
						"referenceName": "ds_emp_out",
						"type": "DatasetReference"
					},
					"name": "oddFile"
				}
			],
			"transformations": [
				{
					"name": "addRowCount"
				},
				{
					"name": "split1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          empID as string,",
				"          name as string,",
				"          country as string,",
				"          salary as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 keyGenerate(output(record_count as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> addRowCount",
				"addRowCount split(record_count%2==0,",
				"     disjoint: false) ~> split1@(evenFile, oddFile)",
				"split1@evenFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     partitionBy('hash', 1)) ~> evenFile",
				"split1@oddFile sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 2) ~> oddFile"
			]
		}
	}
}