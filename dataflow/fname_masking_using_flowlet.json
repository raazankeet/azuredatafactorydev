{
	"name": "fname_masking_using_flowlet",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_emp_in",
						"type": "DatasetReference"
					},
					"name": "inputFile"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FnameMasking",
					"flowlet": {
						"referenceName": "flowlet_fname",
						"type": "DataFlowReference",
						"parameters": {
							"seed": "0"
						}
					}
				}
			],
			"scriptLines": [
				"source(output(",
				"          empID as string,",
				"          name as string,",
				"          country as string,",
				"          salary as string,",
				"          department as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> inputFile",
				"inputFile compose(mapParameter(",
				"          seed = 0",
				"     ),",
				"     mapColumn(",
				"          inputFname = name",
				"     ),",
				"     composition: 'flowlet_fname') ~> FnameMasking@(MaskedName)"
			]
		}
	}
}