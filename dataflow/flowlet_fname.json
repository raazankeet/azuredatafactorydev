{
	"name": "flowlet_fname",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Lnkd_ankitadlsadf",
						"type": "LinkedServiceReference"
					},
					"name": "FirstNameDictionary"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FnameHasher"
				},
				{
					"name": "NameSeeker"
				},
				{
					"name": "inputFname"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     seed as integer (0)",
				"}",
				"input(output(",
				"          inputFname as string",
				"     ),",
				"     order: 0) ~> inputFname",
				"source(output(",
				"          sno as long,",
				"          Fname as string",
				"     ),",
				"     useSchema: false,",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false,",
				"     format: 'delimited',",
				"     fileSystem: 'lookup-dictionaries',",
				"     fileName: 'first_names.csv',",
				"     columnDelimiter: ',',",
				"     escapeChar: '\\\\',",
				"     quoteChar: '\\\"',",
				"     columnNamesAsHeader: true) ~> FirstNameDictionary",
				"inputFname derive(HashFname = mod(crc32(inputFname)+$seed,1000)) ~> FnameHasher",
				"FnameHasher, FirstNameDictionary lookup(HashFname == sno,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> NameSeeker",
				"NameSeeker output(mapColumn(",
				"          inputFname,",
				"          Fname",
				"     )) ~> output1"
			]
		}
	}
}